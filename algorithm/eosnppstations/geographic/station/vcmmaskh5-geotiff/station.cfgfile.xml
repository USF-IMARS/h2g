<NCS_Station name="VIIRS Cloudmask geotiff" verbose_logging="true">
  <group>VIIRS Cloudmask geotiff group1</group>
  <algorithm>
    <installation>{cfg_nisgs_home}/SPA/h2g/wrapper/h2g/installation.xml</installation>
    <version>VIIRS Cloudmask geotiff 1.0</version>
  </algorithm>
  <inputs>
    <product type="drl.npp.viirs.cmip.h5" wrappertype="modis.data"/>
    <product type="drl.npp.viirs.gmodo" wrappertype="modis.geo"/>
  </inputs>
  <setup>
    <Ncs_date getValue="starttime" name="cfg_starttime"/>  
    <Ncs_set name="geotiffname" value="NPP_CMIP_L2.{starttime}.CLOUDMASK.h5.tif"/>
    <Ncs_log text="Name of GEOTIFF is {geotiffname}"/>
    <Ncs_set name="config.type" value="standard"/>
    <Ncs_set name="config.name" value="vcmmaskh5"/>
    <Ncs_set name="output.type" value="geotiff.u8cm"/>
  </setup>
  <outputs>
    <product type="drl.npp.viirs.cmip_maskh5.geotiff" filename="{dataDirectory}{productDirectory}{/}{geotiffname}" wrappertype="modis.h2gout"/>
  </outputs>
</NCS_Station>
